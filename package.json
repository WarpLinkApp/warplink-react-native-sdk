{
  "name": "@warplink/react-native",
  "version": "0.1.0",
  "description": "WarpLink deep linking SDK for React Native",
  "main": "lib/commonjs/index",
  "module": "lib/module/index",
  "types": "lib/typescript/index.d.ts",
  "react-native": "src/index",
  "source": "src/index",
  "files": [
    "src",
    "lib",
    "ios",
    "android",
    "warplink-react-native.podspec",
    "react-native.config.js",
    "!**/__tests__",
    "!**/__mocks__",
    "!**/.*"
  ],
  "scripts": {
    "build": "bob build",
    "lint": "eslint src/",
    "typescript": "tsc --noEmit",
    "test": "jest",
    "prepare": "bob build"
  },
  "keywords": [
    "react-native",
    "deep-linking",
    "deferred-deep-links",
    "warplink",
    "attribution"
  ],
  "repository": {
    "type": "git",
    "url": "https://github.com/AeroDeploy/warplink-react-native-sdk.git"
  },
  "author": "AeroDeploy",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/AeroDeploy/warplink-react-native-sdk/issues"
  },
  "homepage": "https://github.com/AeroDeploy/warplink-react-native-sdk#readme",
  "publishConfig": {
    "registry": "https://registry.npmjs.org/"
  },
  "peerDependencies": {
    "react": ">=18.0.0",
    "react-native": ">=0.71.0"
  },
  "devDependencies": {
    "@types/react": "^18.0.0",
    "@typescript-eslint/eslint-plugin": "^7.0.0",
    "@typescript-eslint/parser": "^7.0.0",
    "eslint": "^8.0.0",
    "@types/jest": "^29.0.0",
    "jest": "^29.0.0",
    "ts-jest": "^29.0.0",
    "react": "^18.0.0",
    "react-native": "^0.75.0",
    "react-native-builder-bob": "^0.30.0",
    "typescript": "^5.0.0"
  },
  "react-native-builder-bob": {
    "source": "src",
    "output": "lib",
    "targets": [
      "commonjs",
      "module",
      [
        "typescript",
        {
          "project": "tsconfig.build.json"
        }
      ]
    ]
  }
}
